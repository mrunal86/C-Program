public class Program
{
	public static void Main()
	{
		Console.WriteLine("Hello World");
		for(int i=0;i<10;i++)
		{
			for(int j=0;j<=i;j++)
			{
				if(j==0 || j== i)
				{
					Console.Write(1);
					Console.Write(" ");
				}
				
				else
				{
					Console.Write(0);
				}
			}
			Console.WriteLine();
		}
	}
}
